{
  "version": 2,
  "buildCommand": "./build.sh",
  "framework": "sveltekit",
  "outputDirectory": ".vercel/output",
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 5 * * *"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    },
    ".svelte-kit/output/server/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
